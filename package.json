{
  "name": "circle-web",
  "version": "0.0.0",
  "description": "Circle Web App",
  "scripts": {
    "start": "coffee server.coffee",
    "test": "jest",
    "debug-test": "mocha debug ./test/spec_helper.coffee ./public/**/*.spec.coffee",
    "setup-dns": "echo '127.0.0.1 local.circlehq.co' >> /etc/hosts && dscacheutil -flushcache",
    "open": "open http://local.circlehq.co:9110/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getcircle/circle-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/getcircle/circle-web/issues"
  },
  "homepage": "https://github.com/getcircle/circle-web",
  "dependencies": {
    "protobufs": "git+ssh://git@github.com/getcircle/protobuf-registry.git#2.17.3",
    "rsvp": "^3.0.18"
  },
  "devDependencies": {
    "gulp": "~3.8.11",
    "coffee-script": "~1.9.1",
    "coffee-reactify": "~3.0.0",
    "gulp-browserify": "~0.5.1",
    "require-dir": "~0.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-watch": "~4.2.4",
    "domready": "~1.0.7",
    "react": "~0.13.1",
    "express": "~4.7.2",
    "compression": "~1.0.9",
    "method-override": "~2.1.2",
    "morgan": "~1.2.2",
    "body-parser": "~1.5.2",
    "gulp-sass": "~1.3.3",
    "gulp-shell": "~0.4.0",
    "coffee-react": "~3.0.1",
    "jsdom": "~4.4.0",
    "q": "~1.2.0",
    "lodash": "~2.4.1",
    "moment": "~2.10.2",
    "fb": "~0.7.0",
    "iz": "~0.7.1",
    "spin": "0.0.1",
    "hammerjs": "~2.0.4",
    "fastclick": "~1.0.3",
    "gulp-uglify": "~1.2.0",
    "gulp-plumber": "~1.0.0",
    "gulp-util": "~3.0.4",
    "node-notifier": "~4.1.2",
    "jest-cli": "~0.4.0",
    "flux-react-router": "~0.6.1",
    "gulp-batch": "~1.0.5",
    "flux": "~2.0.1",
    "superagent": "git+ssh://git@github.com/mhahn/superagent.git"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "scriptPreprocessor": "<rootDir>/jest_preprocessor.js",
    "testFileExtensions": [
      "spec.js",
      "spec.coffee",
      "spec.cjsx"
    ],
    "moduleFileExtensions": [
      "js",
      "coffee",
      "cjsx"
    ]
  }
}
