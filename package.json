{
  "name": "circle-web",
  "version": "0.0.0",
  "description": "Circle Web App",
  "scripts": {
    "build": "npm run clean && webpack --progress --config webpack/local.config.js",
    "build-dev": "npm run clean && webpack --progress --config webpack/dev.config.js",
    "build-prod": "npm run clean && webpack --progress --config webpack/prod.config.js",
    "build-staging": "npm run clean && webpack --progress --config webpack/staging.config.js",
    "clean": "rm -rf static/dist",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "open": "open http://local.lunohq.com:9110/",
    "package": "npm run build && docker build -t lunohq/web-local .",
    "package-dev": "npm run build-dev && docker build -t lunohq/web-dev .",
    "package-prod": "npm run build-prod && docker build -t lunohq/web-prod .",
    "package-staging": "npm run build-staging && docker build -t lunohq/web-staging .",
    "setup-dns": "echo '127.0.0.1 local.lunohq.com' >> /etc/hosts && dscacheutil -flushcache",
    "start": "node bin/server.js",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "start-docker": "npm run package-dev && docker-compose up -d",
    "test": "mocha",
    "test-watch": "npm test -- --watch --growl",
    "watch-client": "node ./node_modules/better-npm-run watch-client"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "local",
        "PORT": 3000,
        "REMOTE_API_ENDPOINT": "http://`docker-machine ip dev`:8000/v1/",
        "AUTHENTICATION_TOKEN_COOKIE_DOMAIN": ".local.lunohq.com",
        "REDIS_URL": "redis://`docker-machine ip dev`:6379",
        "SESSION_SECRET": "shhhh"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "DEVHOST": "`docker-machine ip dev`"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getcircle/circle-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/getcircle/circle-web/issues"
  },
  "homepage": "https://github.com/getcircle/circle-web",
