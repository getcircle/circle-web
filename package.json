{
  "name": "circle-web",
  "version": "0.0.0",
  "description": "Circle Web App",
  "scripts": {
    "start": "babel-node server.js",
    "test": "jest",
    "debug-test": "mocha debug ./test/spec_helper.coffee ./public/**/*.spec.coffee",
    "setup-dns": "echo '127.0.0.1 local.circlehq.co' >> /etc/hosts && dscacheutil -flushcache",
    "open": "open http://local.circlehq.co:9110/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getcircle/circle-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/getcircle/circle-web/issues"
  },
  "homepage": "https://github.com/getcircle/circle-web",
  "dependencies": {
    "protobufs": "git+ssh://git@github.com/getcircle/protobuf-registry.git#2.17.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "alt": "^0.16.10",
    "babel": "^5.5.8",
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.2",
    "body-parser": "~1.5.2",
    "browserify": "^10.2.4",
    "compression": "~1.0.9",
    "domready": "~1.0.7",
    "express": "~4.7.2",
    "fastclick": "~1.0.3",
    "fb": "~0.7.0",
    "flux": "~2.0.1",
    "gulp": "^3.9.0",
    "gulp-batch": "~1.0.5",
    "gulp-plumber": "~1.0.0",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~1.3.3",
    "gulp-shell": "~0.4.0",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "~3.0.4",
    "gulp-watch": "~4.2.4",
    "hammerjs": "~2.0.4",
    "iz": "~0.7.1",
    "jest-cli": "~0.4.0",
    "jsdom": "~4.4.0",
    "keymirror": "^0.1.1",
    "lodash": "^3.9.3",
    "material-ui": "^0.9.1",
    "method-override": "~2.1.2",
    "moment": "~2.10.2",
    "morgan": "~1.2.2",
    "node-notifier": "~4.1.2",
    "q": "~1.2.0",
    "react": "~0.13.1",
    "react-mixin": "^1.3.1",
    "react-tap-event-plugin": "^0.1.7",
    "require-dir": "~0.1.0",
    "spin": "0.0.1",
    "superagent": "git+ssh://git@github.com/mhahn/superagent.git",
    "vinyl-source-stream": "^1.1.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "spec.js",
      "spec.jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
